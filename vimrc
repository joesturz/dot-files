syntax on
set term=builtin_ansi 
set backspace=indent,eol,start
set tabstop=2
set expandtab
set shiftwidth=2
set number
highlight LineNr term=bold cterm=NONE ctermfg=LightBlue ctermbg=NONE gui=NONE guifg=DarkGrey guibg=NONE
highlight Keyword term=bold cterm=NONE ctermfg=Blue ctermbg=NONE gui=NONE guifg=DarkGrey guibg=NONE
highlight Conditional term=bold cterm=NONE ctermfg=Green ctermbg=NONE gui=NONE guifg=DarkGrey guibg=NONE
highlight Statement term=bold cterm=NONE ctermfg=Green ctermbg=NONE gui=NONE guifg=DarkGrey guibg=NONE
highlight Comment term=bold cterm=NONE ctermfg=LightYellow ctermbg=NONE gui=NONE guifg=DarkGrey guibg=NONE
